{
  "name": "e-commerce",
  "version": "1.0.0",
  "private": true,
  "description": "Full-stack E-commerce Web App",
  "type": "module",
  "scripts": {
    "client": "npm --prefix client start",
    "server": "nodemon server/app.js",
    "dev": "concurrently -k -n CLIENT,SERVER \"npm run client\" \"npm run server\""
  },
  "nodemonConfig": {
    "watch": [
      "backend/app.js",
      "backend/routes",
      "backend/controllers",
      "backend/models"
    ],
    "ext": "js,json",
    "ignore": [
      "client",
      "node_modules"
    ],
    "exec": "node"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/styled-engine-sc": "^7.1.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.10.0",
    "body-parser": "^2.2.0",
    "bootstrap": "^4.1.3",
    "cloudinary": "^2.7.0",
    "config": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^6.12.6",
  "mongodb": "^4.15.0",
  "bson": "^4.7.2",
  "multer-gridfs-storage": "^5.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "p-limit": "^6.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-inner-image-zoom": "^4.0.1",
    "react-range-slider-input": "^3.2.1",
    "react-router-dom": "^7.6.2",
    "react-scripts": "5.0.1",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.1.19",
    "swiper": "^11.2.8",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
